{
  "name": "musubii",
  "version": "8.0.0",
  "description": "Simple CSS Framework for JP",
  "main": "dist/musubii.css",
  "license": "CC0-1.0",
  "homepage": "https://musubii.qranoko.jp",
  "repository": "https://github.com/qrac/musubii",
  "bugs": "https://github.com/qrac/musubii/issues",
  "keywords": [
    "css",
    "framework",
    "japanese",
    "musubii"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "npm run build:concat && npm run build:minify",
    "build:concat": "postcss ./src/*.css -d ./dist --no-map",
    "build:minify": "cleancss --batch --batch-suffix '.min' dist/*.css !dist/*.min.css -d",
    "site": "vite build",
    "preview": "vite preview --port 8080",
    "clean": "trash ./dist ./test/dist"
  },
  "prettier": {
    "semi": false
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Edge versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 iOS versions",
    "last 1 ChromeAndroid versions"
  ],
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.3",
    "clean-css-cli": "^5.5.0",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.0.2",
    "postcss-sort-media-queries": "^4.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.7"
  }
}
