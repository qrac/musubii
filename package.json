    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "autoprefixer": "^9.7.5",
    "babel-plugin-root-import": "^6.5.0",
    "clean-css-cli": "^4.3.0",
    "concat": "^1.0.3",
    "cpx": "^1.5.0",
    "css-vars-ponyfill": "^2.2.1",
    "highlight.js": "^10.4.1",
    "js-beautify": "^1.10.3",
    "next": "^9.3.3",
    "next-compose-plugins": "^2.2.0",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-sort-media-queries": "^1.4.23",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
