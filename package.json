{
  "name": "musubii-monorepo",
  "version": "7.2.0",
  "private": true,
  "homepage": "https://musubii.qranoko.jp",
  "keywords": [
    "css",
    "framework",
    "japanese",
    "musubii"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "build": "npm run build:main",
    "build:src": "npm run build:main-src",
    "build:main": "cd ./packages/musubii && npm run build",
    "build:main-src": "cd ./packages/musubii && npm run build:src",
    "play": "vite ./playground/_default",
    "up": "npx npm-check-updates --deep",
    "up:u": "npx npm-check-updates --deep -u",
    "rev": "npx npm-replace-versions",
    "release": "npm run release:main",
    "release:main": "cd ./packages/musubii && npm publish",
    "clean": "npm run clean:main",
    "clean:main": "cd ./packages/musubii && npm run clean"
  },
  "workspaces": [
    "docs",
    "packages/*",
    "playground/*"
  ],
  "devDependencies": {
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@vitejs/plugin-react": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "vite": "^4.4.9",
    "vite-plugin-banner": "^0.7.1"
  }
}
