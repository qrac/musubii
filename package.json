{
  "name": "musubii-monorepo",
  "version": "8.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://musubii.qranoko.jp",
  "keywords": [
    "css",
    "framework",
    "japanese",
    "musubii"
  ],
  "author": {
    "name": "Qrac",
    "url": "https://qrac.jp"
  },
  "organization": {
    "name": "QRANOKO",
    "url": "https://qranoko.jp"
  },
  "scripts": {
    "docs": "minista ./docs",
    "docs-build": "minista build ./docs",
    "docs-build-with-demo": "minista build ./docs && npm run play-build-for-docs",
    "docs-preview": "minista preview ./docs",
    "build": "npm run build:main",
    "build-src": "npm run build-src:main",
    "build:main": "cd ./packages/musubii && npm run build",
    "build-src:main": "cd ./packages/musubii && npm run build-src",
    "play": "vite dev ./playground/_default",
    "play-build": "vite build ./playground/_default",
    "play-build-for-docs": "vite build ./playground/_default --base=/demo/ --outDir=../../docs/dist/demo",
    "play-preview": "vite preview ./playground/_default",
    "up": "npx npm-check-updates --deep",
    "up:u": "npx npm-check-updates --deep -u",
    "rev": "npx npm-replace-versions",
    "release": "npm run release:main",
    "release:main": "cd ./packages/musubii && npm publish",
    "clean": "npm run clean:main",
    "clean:main": "cd ./packages/musubii && npm run clean"
  },
  "workspaces": [
    "docs",
    "packages/*",
    "playground/*"
  ],
  "devDependencies": {
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@vitejs/plugin-react": "^4.2.1",
    "clsx": "^2.1.0",
    "esbuild": "^0.20.2",
    "minista": "^4.0.0-alpha.6",
    "minista-plugin-entry": "^4.0.0-alpha.6",
    "minista-plugin-mdx": "^4.0.0-alpha.6",
    "minista-plugin-ssg": "^4.0.0-alpha.6",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-svgr": "^4.2.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.14.1"
  }
}
